{
  "name": "astro-template",
  "version": "0.0.0",
  "description": "",
  "author": "YOUR_NAME_HERE",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w . --plugin=prettier-plugin-astro",
    "format:check": "prettier -c . --plugin=prettier-plugin-astro",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:strict": "eslint --max-warnings 0 .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "test": "uvu -r esbuild-register tests -i utils",
    "build:types": "astro sync",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,json,md,yml}": [
      "prettier -w"
    ],
    "**/*.astro": [
      "prettier -w --plugin=prettier-plugin-astro"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "browserslist": [
    "last 2 versions",
    ">0.5% and not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^3.5.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@nanostores/preact": "^0.5.2",
    "astro": "^4.13.1",
    "classcat": "^5.0.5",
    "nanostores": "^0.11.2",
    "postcss": "^8.4.41",
    "postcss-preset-env": "^10.0.0",
    "preact": "^10.23.1",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.2",
    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "esbuild": "^0.23.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-foxkit": "3.2.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.5.4",
    "uvu": "^0.5.6"
  }
}
